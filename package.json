{
  "name": "faucet-sdk",
  "version": "0.29.0",
  "description": "Typescript SDK for easy implementation of Faucet.",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json --rootDir=./src",
    "dev": "webpack --mode development",
    "webpack": "webpack --mode production",
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "reversion": "npm --no-git-tag-version version minor",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "husky": "npx husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abigger87/faucet-sdk.git"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "rari",
    "faucet",
    "faucet-sdk",
    "decentralized finance",
    "defi",
    "ethereum"
  ],
  "author": "Andreas Bigger <bigger@usc.edu>",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/abigger87/faucet-sdk/issues"
  },
  "homepage": "https://github.com/abigger87/faucet-sdk#readme",
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/preset-env": "^7.11.5",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.19.2",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "assert": "^2.0.0",
    "babel-loader": "^8.2.2",
    "buffer": "^5.6.0",
    "core-js": "^3.6.5",
    "electron": "^10.1.5",
    "global": "^4.4.0",
    "husky": "^5.1.1",
    "jest": "^26.6.3",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "regenerator-runtime": "^0.13.7",
    "stream-http": "^3.1.1",
    "terser-webpack-plugin": "^5.0.0",
    "ts-jest": "^26.5.2",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.3",
    "url": "^0.11.0",
    "util": "^0.12.3",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "ethers": "^5.0.26",
    "prettier": "^2.2.1",
    "swarm-js": "^0.1.40",
    "ts-loader": "^8.0.17",
    "web3": "1.3.3",
    "web3-utils": "^1.3.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && yarn lint && npm --no-git-tag-version version minor && git add ."
    }
  },
  "lint-staged": {
    "*.ts": [
      "tslint",
      "prettier --write",
      "git add"
    ]
  }
}
